{
    "name": "{{ remote_name }}",
    "host": "{{ ssh_ip }}",
    "protocol": "sftp",
    "port": 22,
    "username": "{{ ssh_username }}",
    "remotePath": "{{ remote_project_directory }}",
    "privateKeyPath": "{{ local_private_key_path }}",
    "uploadOnSave": false,
    "useTempFile": false,
    "openSsh": false,
    "ignore":[
        ".git",
        ".DS_Store"
    ]    
}
